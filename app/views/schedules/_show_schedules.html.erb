<div class = "well">
	<table class="table table-condensed table-hover">
		<tbody>
			<tr class="warning">
				<td>行程</td>
				<td>描述</td>
				<td>開始時間</td>
				<td>結束時間</td>
			</tr>
		<% @schedules.each do |schedule| %>
			<tr>
				<td><%= schedule.title %></td>
				<td><%= truncate(schedule.description , length: 20) %></td>
				<td><%= schedule.starttime.strftime('%Y-%m-%d %H:%M') %></td>
				<td><%= schedule.endtime.strftime('%Y-%m-%d %H:%M') %></td>
			</tr>
		<% end%>
		</tbody>
	</table>
</div>